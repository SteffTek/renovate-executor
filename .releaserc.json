{
    "branches": [
        "main",
        {
            "name": "develop",
            "prerelease": true
        }
    ],
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/github",
            {
                "assets": [
                    {
                        "path": "dist/worker.js",
                        "label": "Worker"
                    },
                    {
                        "path": "lang.json",
                        "label": "Language File"
                    },
                    {
                        "path": "openapi.json",
                        "label": "OpenAPI File"
                    },
                    {
                        "path": "drizzle.zip",
                        "label": "Migrations"
                    }
                ]
            }
        ],
        [
            "@saithodev/semantic-release-backmerge",
            {
                "backmergeBranches": [
                    {
                        "from": "main",
                        "to": "develop"
                    }
                ]
            }
        ]
    ]
}